{
  "extends": "tslint:recommended",
  "rulesDirectory": [
    "src/"
  ],
  "rules": {
    "max-line-length": {
      "options": [
        120
      ]
    },
    "semicolon": [
      true,
      "never"
    ],
    "quotemark": [
      true,
      "single",
      "jsx-double"
    ],
    "interface-name": [
      true
    ],
    "member-access": [
      true
    ],
    "space-before-function-paren": [
      false
    ],
    "object-literal-sort-keys": false,
    "eofline": false,
    "new-parens": true,
    "no-arg": true,
    "no-bitwise": true,
    "no-conditional-assignment": true,
    "no-consecutive-blank-lines": false,
    "no-console": false
  },
  "jsRules": {
    "max-line-length": {
      "options": [
        120
      ]
    }
  }
}